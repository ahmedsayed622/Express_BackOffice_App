{
  "name": "express-backoffice-app",
  "version": "1.0.0",
  "description": "Express BackOffice Application with Oracle Database",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "start:dev": "cross-env NODE_ENV=development node src/server.js",
    "start:test": "cross-env NODE_ENV=test node src/server.js",
    "start:prod": "cross-env NODE_ENV=production node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "db:sync": "node scripts/sync-db.js",
    "verify:db": "node scripts/verify-connectivity.js",
    "gen:api-key": "node scripts/generate-api-key.js"
  },
  "keywords": [
    "express",
    "oracle",
    "sequelize",
    "backoffice",
    "api"
  ],
  "author": "Ahmed Sayed",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dotenv": "^16.3.1",
    "dotenv-flow": "^4.1.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "oracledb": "^6.9.0",
    "sequelize": "^6.35.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
